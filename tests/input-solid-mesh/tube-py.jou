#!python
import cubit

cubit.cmd('reset')
cubit.cmd('set geometry engine acis')


b = 1
h = 1
t = 10

nx = 1
ny = 1
nz = 3

# create geometry
cubit.cmd('create Cylinder height 10 radius 25e-2')

# set mesh size
cubit.cmd('curve 1  interval 6')
cubit.cmd('curve 2  interval 6')
cubit.cmd('surface 1  size 1')
cubit.cmd('mesh volume 1')

# set blocks and sets
cubit.cmd('reset block')
cubit.cmd('block 1 volume 1')
cubit.cmd('block 1 element type HEX8')

for i in range(3):
    cubit.cmd('nodeset ' + str(i+1) + ' surface ' + str(i+1))

for i in range(1):
    cubit.cmd('nodeset ' + str(3+i+1) + ' curve ' + str(i+1))

cubit.cmd('nodeset 5 volume 1')

# export mesh
cubit.cmd('export mesh "tube.exo" overwrite')

print("Last Python line")


